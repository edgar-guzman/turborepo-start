{
  "extends": "@edgarguzman/typescript/next.json",
  "compilerOptions": {
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],

    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src", "next-env.d.ts", "next.config.mjs", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
